<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record id="view_partner_fiskal_tag_tree" model="ir.ui.view">
        <field name="name">view_partner_fiskal_tag_tree</field>
        <field name="model">res.partner.fiskal.tag</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_partner_fiskal_tag_form" model="ir.ui.view">
        <field name="name">view_partner_fiskal_tag_form</field>
        <field name="model">res.partner.fiskal.tag</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
                <notebook>
                    <page string="Partners">
                        <field name="partner_ids"/>
                    </page>
                    <page string="Required fields">
                        <field name="required_field_ids"/>
                    </page>
                </notebook>
            </form>
        </field>
    </record>
    <record id="action_partner_fiskal_tags" model="ir.actions.act_window">
        <field name="name">Partner fiskal tags</field>
        <field name="res_model">res.partner.fiskal.tag</field>
        <field name="view_mode">tree,form</field>
        <field name="view_type">form</field>
    </record>
    <menuitem id="menu_partner_fiskal_tag"
              groups="partner_fiskal_responsible.group_fiskal_responsible_manager"
              parent="account.account_account_menu"
              action="action_partner_fiskal_tags"/>
              
    
    
</odoo>
