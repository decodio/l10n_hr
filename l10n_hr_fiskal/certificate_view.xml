<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_certificate_form_fiskal" model="ir.ui.view">
            <field name="inherit_id" ref="crypto.view_certificate_form"/>
            <field name="name">crypto.certificate.form.fiskal</field>
            <field name="model">crypto.certificate</field>
            <field name="arch" type="xml">
                <field name="pairkey_id" position="before">
                    <field name="pfx_certificate"/>
                    <field name="group_id"/>
                </field>
                <field name="status" position="before">
                    <field name="cert_type"/>
                    <field name="company_id"/>
                </field>
            </field>
        </record>

    </data>
</openerp>
